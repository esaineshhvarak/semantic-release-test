{
  "name": "semantic-release",
  "version": "0.0.0-development",
  "private": true,
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "commit": "git-cz",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release --branches dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
